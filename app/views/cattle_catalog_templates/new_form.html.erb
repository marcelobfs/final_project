<h1>
  Add a new cattle catalog
</h1>


<hr>

<form action="/create_cattle_catalog" method="post">

  <div class="form-group">
    <label for="first_weight">
      First weight
    </label>

    <input type="text" id="first_weight" name="first_weight" class="form-control">
  </div>

  <div class="form-group">
    <label for="source">
      Source
    </label>

    <input type="text" id="source" name="source" class="form-control">
  </div>

  <div class="form-group">
    <label for="race">
      Race
    </label>

    <input type="text" id="race" name="race" class="form-control">
  </div>

  <div class="form-group">
    <label for="gender">
      Gender
    </label>

    <input type="text" id="gender" name="gender" class="form-control">
  </div>

  <div class="form-group">
    <label for="price_per_kilo">
      Price per kilo
    </label>

    <input type="text" id="price_per_kilo" name="price_per_kilo" class="form-control">
  </div>

  <div class="form-group">
    <label for="father">
      Father
    </label>

    <input type="text" id="father" name="father" class="form-control">
  </div>

  <div class="form-group">
    <label for="earring_id">
      Earring
    </label>

    <input type="text" id="earring_id" name="earring_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="purchase_or_weaning_date">
      Purchase or weaning date
    </label>

    <input type="text" id="purchase_or_weaning_date" name="purchase_or_weaning_date" class="form-control">
  </div>

  <div class="form-group">
    <label for="mother_earring_id">
      Mother earring
    </label>

    <input type="text" id="mother_earring_id" name="mother_earring_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="birthdate">
      Birthdate
    </label>

    <input type="text" id="birthdate" name="birthdate" class="form-control">
  </div>

  <div class="form-group">
    <label for="status">
      Status
    </label>

    <input type="text" id="status" name="status" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create cattle catalog
  </button>
</form>

<hr>

<a href="/cattle_catalogs">
  Go back
</a>
